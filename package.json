{
  "name": "mobile-node-select",
  "version": "2.2.0",
  "description": "基于vue的移动端部门(节点树)选择组件/The vue-based mobile department (node tree) selection component.",
  "main": "./lib/nodeselect.common.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:hyt10290330/mobile-node-select.git"
  },
  "author": "hythandsome",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name nodeselect --dest lib packages/index.js"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "better-scroll": "^1.15.2",
    "core-js": "^2.6.9",
    "postcss-adaptive": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem-exclude": "0.0.6",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-adaptive": {
        "remUnit": 75,
        "autoRem": true
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
